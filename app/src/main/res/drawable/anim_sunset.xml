			<clip-path android:pathData="M0,7 H24, V18.5, H0Z"/>
			<path
				android:strokeColor="@android:color/white"
				android:strokeWidth="1"
				android:pathData="M 18 18 A 6 6 0 0 1 12 24 A 6 6 0 0 1 6 18 A 6 6 0 0 1 12 12 A 6 6 0 0 1 18 18 Z"/>
