<vector
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:width="24dp"
	android:height="24dp"
	android:autoMirrored="true"
	android:viewportWidth="24"
	android:viewportHeight="24">
	<group
		android:scaleX="0.75"
		android:scaleY="0.75"
		android:translateX="-1.25"
		android:translateY="3.25">
		<path
			android:fillColor="@color/moon_fill"
			android:strokeWidth="1"
			android:strokeColor="@color/moon_stroke"
			android:pathData="@string/moon_path"/>
	</group>
	<path
		android:fillColor="@color/cloud_dark_fill"
		android:strokeWidth="1"
		android:strokeColor="@color/cloud_dark2_stroke"
		android:pathData="@string/cloud_path"/>
	<group android:translateX="-2.25">
		<path
			android:fillColor="@color/cloud_dark_fill"
			android:pathData="@string/ice_pellet_path"/>
	</group>
	<group android:translateX="2.25">
		<path
			android:fillColor="@color/cloud_dark_fill"
			android:pathData="@string/ice_pellet_path"/>
	</group>
</vector>
